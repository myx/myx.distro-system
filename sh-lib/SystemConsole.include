#!/usr/bin/env bash
# ^^^ for syntax checking in the editor only

case "$1" in 
	--completion-*)
		. "$MDSC_ORIGIN/myx/myx.distro-system/sh-lib/SystemConsole.CompletionCommands.include"
		set +e ; return 1
	;;
	''|--help)
		shift
		( . "$MDSC_ORIGIN/myx/myx.distro-system/sh-lib/HelpConsoleDistro.include" )
		set +e ; return 1
	;;
	*)
		echo "DistroSourceConsole: Invalid argument: $1" >&2
		set +e ; return 1
	;;
esac
