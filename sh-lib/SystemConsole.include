#!/usr/bin/env bash
# ^^^ for syntax checking in the editor only

set -e 
case "$1" in 
	--completion-*)
		( . "$MDLT_ORIGIN/myx/myx.distro-system/sh-lib/SystemConsole.CompletionCommands.include" )
		return 0
	;;
	''|--help)
		shift
		( . "$MDLT_ORIGIN/myx/myx.distro-system/sh-lib/Help.ConsoleDistro.include" )
		return 0
	;;
	*)
		echo "SourceConsole: Invalid argument: $1" >&2
		set +e ; return 1
	;;
esac
